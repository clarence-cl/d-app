.icon {
    width: 1em;
    height: 1em;
    vertical-align: -0.15em;
    fill: currentColor;
    overflow: hidden;
    font-size: 3rem;
}
@font-face {
    font-family: 'iconfont';
    src: url('../../../common/fontclass/iconfont.eot');
    src: url('../../../common/fontclass/iconfont.eot?#iefix') format('embedded-opentype'),
    url('../../../common/fontclass/iconfont.woff') format('woff'),
    url('../../../common/fontclass/iconfont.ttf') format('truetype'),
    url('../../../common/fontclass/iconfont.svg#iconfont') format('svg');
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 3rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}

.font_family {
    font-family:"font_family" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
html,
body {
    background-color: #f6f6f6;
    font-family: "PingFang SC";
}

.setTop {
    height: 11.6rem;
    font-size: 2.5rem;
    line-height: 11.6rem;
    padding-left: 8rem;
    background-color: #FFFFFF;
    position: relative;
}

.setTopContent {
    width: 21.4rem;
    height: 4rem;
    border: 1px solid #333333;
    border-radius: 5px;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 1rem;
    margin: auto;
    display: flex;
    display: -webkit-flex;
    font-size: 1.6rem;
}

.setTopContent>div {
    display: flex;
    display: -webkit-flex;
    flex: 1;
    -webkit-flex: 1;
    justify-content: center;
    -webkit-justify-content: center;
    align-items: center;
    -webkit-align-items: center;
}

.setContent {
    margin: 0 8rem;
}

.setContent>div {
    height: 6.5rem;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    border-top: 1px solid #e5e5e5;
    padding: 0 3rem 0 1.5rem;
}

.setContent>div>div {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
}

.setContent>div>div>span {
    display: inline-block;
    padding-left: 1.4rem;
    font-size: 1.8rem;
}

.setContent>div>div>div {
    display: inline-block;
    margin-left: 3.6rem;
    font-size: 1.8rem;
    color: #999999;
}

.setContent>div>div>input {
    margin-left: 3.6rem;
    font-size: 1.8rem;
    color: #999999;
}

.tab {
    background-color: #FFFFFF;
    color: #666666;
}

.sel {
    background-color: #333333;
    color: #FFFFFF;
}

.esBtn {
    width: 53rem;
    height: 4rem;
    margin: 9rem auto 2.4rem auto;
    border-radius: 100px;
    border: 1px solid #E8E8E8;
    background-color: #FFFFFF;
    text-align: center;
    line-height: 4rem;
    color: #393939;
    font-size: 2.2rem;
}

.setMask {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.4);
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    -webkit-justify-content: center;
    align-items: center;
    -webkit-align-items: center;
    z-index: 1;
}

.setMask>div {
    z-index: 2;
}

.setPopup {
    position: relative;
    width: 42rem;
    border-radius: 5px;
    background-color: #FFFFFF;
    padding: 2rem 0 3rem 0;
    text-align: center;
}

.setPopupTop {
    width: 4rem;
    height: 0.6rem;
    border-radius: 100px;
    background: -webkit-linear-gradient(30deg, #00abb5, #00abb6, #07afb9);
    background: linear-gradient(30deg, #00abb5, #00abb6, #07afb9);
    margin: auto;
}

.setPopup>input {
    width: 38.3rem;
    height: 5.2rem;
    border-radius: 2px;
    background-color: #f1f1f1;
    margin-top: 4rem;
    font-size: 1.6rem;
    outline: none;
    border: 0;
}

.setPopupBottom {
    width: 28.5rem;
    height: 4.5rem;
    margin: 4.7rem auto 0 auto;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}

.setPopupBottom>div {
    width: 13rem;
    height: 100%;
    border-radius: 100px;
    text-align: center;
    line-height: 4.5rem;
    background-color: #1ada95;
    color: #FFFFFF;
    font-size: 2.2rem;
}

.setContent .photo {
    width: 6rem;
    height: 6rem;
    right: 0;
    /*display: none;*/
}

.setContent .photo img {
    width: 100%;
    height: 100%;
    border-radius: 6rem;
}
.skin{
    position: absolute;
    width: 12rem;
    background-color: rgba(1, 169, 223, 0.2);
    border-radius: 4rem 0 0 4rem;
    font-size: 1.3rem;
    right: 0;
    line-height: 1;
    top: 4.5rem;
    padding: 0.5rem;
    padding-left: 1rem;
}
.skin .icon{
    font-size: 2.5rem;
}
.skin span{
    font-size: 1.6rem;
    color: #01a9df;
    vertical-align:0.2rem;
}
@media screen and (max-width: 750px) {
  .setTop{
    padding-top: 1.5rem;
  }
  .skin{
    top:6rem;
  }
}
